apiVersion: types.federation.k8s.io/v1alpha1
kind: FederatedService
metadata:
  name: mongo
  labels:
    name: mongo
spec:
  template:
    metadata:
      labels:
        name: mongo
    spec:
      type: ClusterIP
      ports:
        - port: {{Â var_mongo_db_external_port }}
          targetPort: {{ var_mongo_db_internal_port }}
          protocol: TCP
      selector:
        name: mongo
  placement:
    clusterNames:
    {% for cluster in var_cluster_info.name -%}
     - {{ cluster }}
    {% endfor %}
